<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.q2i.q2iprov">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginCASA"
            android:label="@string/title_activity_login_cas" />
        <activity
            android:name=".LoginOARS"
            android:label="@string/title_activity_login_oars" />
        <activity android:name=".Preference" />
        <activity
            android:name=".MainProviderViewCASA"
            android:label="@string/title_activity_main_provider_view_cas" />
        <activity
            android:name=".MainCASAtab"
            android:label="@string/title_activity_main_casatab"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".DrawerCasa"
            android:label="@string/title_activity_drawer_casa"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainProvViewOArs"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainProvActivityOArs"
            android:label="@string/title_activity_main_prov_oars"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>